<div class="flex-container">
<h1>{{title}}</h1>
<pre>Welcome, {{username}}!</pre>
<a type="button" name="newpost" href="/post/{{username}}">create new post</a>
{{#each posts}}
  <ul class="flex-posts">
    <li>#{{this.id}}</li>
    <li><b>{{this.users_username}}</b></li>
    <li><img src={{this.imageUrl}} alt="{{username}}" style="width:300px;height:300px;"></li>
    <li>{{this.caption}}</li>
  </ul>
  {{/each}}
</div>
